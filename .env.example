# ABV.dev API Key (required)
VITE_ABV_API_KEY=sk_abv_xxx

# Story Protocol API Key (may be optional if ABV handles registration)
VITE_STORY_API_KEY=sk_story_xxx

# Coinbase Embedded Wallets Client ID (optional)
VITE_COINBASE_CLIENT_ID=cb_client_xxx

# Yakoa API Key for originality checks (optional)
VITE_YAKOA_API_KEY=sk_yakoa_xxx

# App URL for deployment
VITE_APP_URL=https://your-deployment-url.vercel.app

# Development mode - use mock APIs instead of real ones
VITE_USE_MOCK=true
